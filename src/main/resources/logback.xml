<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE logback>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d %p %c %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="jp" level="debug" />

  <root>
    <appender-ref ref="STDOUT" />
  </root>
</configuration>

